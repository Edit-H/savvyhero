<h1>This is the contact page</h1>


<h3>Please register for an account.</h3>

<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <p><%= e %></p>
    <% end %>
<% end %>

<form action="/users" method="POST">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
        <p><label>Name <input type="text" name="user[first_name]"></label></p>
        <p><label>Name <input type="text" name="user[last_name]"></label></p>
        <p><label>Email <input type="text" name="user[email]"></label></p>
        <p><label>Password <input type="password" name="user[password]"></label></p>
        <p><label>Password Confirmation <input type="password" name="user[password_confirmation]"></label></p>
        <p><input type="submit" value="Join"></p>
</form>